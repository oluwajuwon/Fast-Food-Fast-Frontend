{
  "name": "fast-food-fast-frontend",
  "version": "1.0.0",
  "description": "A food delivery service application for a restaurant, to enable a restaurant manage customer orders better and deliver fast food items to customers in a 'Split Second",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.prod.js --mode production && npm run build:server ",
    "build:server": "babel src/server.js -d dist",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "heroku-postbuild": "npm run build",
    "test:client": "jest",
    "start:dev": "webpack-dev-server --config ./webpack.config.dev.js --mode development",
    "start:prod": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oluwajuwon/Fast-Food-Fast-Frontend.git"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "**/components/**",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/*.config.{dev|prod}.js",
      "!**/src/index.jsx",
      "!**/src/routes.jsx",
      "!**/src/server.js",
      "!**/*.{snap}"
    ],
    "testRegex": "./__tests__/.*.test.js$",
    "rootDir": ".",
    "setupTestFrameworkScriptFile": "./__tests__/main.js",
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "keywords": [
    "food",
    "order"
  ],
  "author": "Oluwajuwon Fagbohungbe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oluwajuwon/Fast-Food-Fast-Frontend/issues"
  },
  "homepage": "https://github.com/oluwajuwon/Fast-Food-Fast-Frontend#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "history": "^4.7.2",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.1",
    "react-testing-library": "^5.4.4",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "coveralls": "^3.0.2",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-toastify": "^4.5.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4"
  }
}
