{
  "name": "fast-food-fast-frontend",
  "version": "1.0.0",
  "description": "A food delivery service application for a restaurant, to enable a restaurant manage customer orders better and deliver fast food items to customers in a 'Split Second",
  "main": "index.js",
  "scripts": {
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "test:client": "jest",
    "start-dev": "webpack-dev-server --config ./webpack.config.dev.js --mode development",
    "start-prod": "webpack --config ./webpack.config.prod.js --mode production && nodemon ./app/app.js --exec babel-node --"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oluwajuwon/Fast-Food-Fast-Frontend.git"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "**/components/**",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/client/dist/**",
      "!**/*.config.{dev|prod}.js",
      "!**/src/index.jsx"
    ],
    "testRegex": "./__tests__/.*.test.js",
    "rootDir": ".",
    "setupTestFrameworkScriptFile": "./__tests__/main.js"
  },
  "keywords": [
    "food",
    "order"
  ],
  "author": "Oluwajuwon Fagbohungbe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oluwajuwon/Fast-Food-Fast-Frontend/issues"
  },
  "homepage": "https://github.com/oluwajuwon/Fast-Food-Fast-Frontend#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "coveralls": "^3.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
